{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react-native",
      "lib": ["es2020","dom"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "target": "esnext",
      "paths": {
        "@assets": ["./src/assets"],
        "@config": ["./src/config"],
        "@mylib": ["./src/mylib"],
        "@screen": ["./src/screen"],
        "@service": ["./src/service"],
        "@navigation": ["./src/navigation"],
        "@utils": ["./src/utils"],
  
  
        
      },
    },
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ]
}